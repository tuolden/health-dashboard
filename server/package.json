{
  "name": "health-dashboard-server",
  "version": "1.0.0",
  "description": "GraphQL API server for Health Dashboard with webhook integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "graphql",
    "apollo-server",
    "health-dashboard",
    "webhooks",
    "typescript"
